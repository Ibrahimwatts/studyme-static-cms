{
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/config.yml",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/yaml"
        }
      ]
    }
  ]
}